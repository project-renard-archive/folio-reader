#!/usr/bin/env perl

BEGIN {#{{{
	$ENV{PERL_DL_NONLAZY} = 1;
	$|=1; # TODO
}#}}}
use strict;
use FindBin qw($Bin);
use lib "$Bin/../lib";
use forks;

use Folio::ThreadPool;
my $pool = Folio::ThreadPool->new;
$pool->start;

#sub clean_up {
	# TODO
#}
#$SIG{INT} = 'clean_up';

require Folio::App;
$ENV{PERL_DL_NONLAZY} = 0;
exit Folio::App->new(pool => $pool)->run(\@ARGV);

